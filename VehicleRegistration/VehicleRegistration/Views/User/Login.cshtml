@using VehicleRegistration.Models
@model UserModel
<!DOCTYPE html>
@{
    ViewBag.Title = "Login";
}
<style>
    body,html{margin:0;padding:0;height:100%}.user_card{height:450px;width:350px;margin-top:10%;margin-bottom:auto;background:#fff;position:relative;display:flex;justify-content:center;flex-direction:column;padding:10px;box-shadow:0 4px 8px 0 rgba(0,0,0,.2),0 6px 20px 0 rgba(0,0,0,.19);-webkit-box-shadow:0 4px 8px 0 rgba(0,0,0,.2),0 6px 20px 0 rgba(0,0,0,.19);-moz-box-shadow:0 4px 8px 0 rgba(0,0,0,.2),0 6px 20px 0 rgba(0,0,0,.19);border-radius:10px}.brand_logo_container{position:absolute;height:170px;width:170px;top:-75px;border-radius:50%;background:-webkit-linear-gradient(left,#468257,#468257,#468257);padding:10px;text-align:center}.brand_logo{height:150px;width:150px;border-radius:50%;border:5px #303f54}.form_container{margin-top:100px}.login_btn{width:100%;background:-webkit-linear-gradient(left,#3eb086,#179f93,#3eb086);color:#fff!important}.login_btn:focus{box-shadow:none!important;outline:0!important}.login_container{padding:0 2rem}.input-group-text{color:#303f54!important;border:0!important;border-radius:.25rem 0 0 .25rem!important}.input_pass:focus,.input_user{box-shadow:none!important;outline:0!important}h5{color:#303f54}.custom-checkbox .custom-control-input:checked~.custom-control-label::before{background-color:#3eb086!important}
</style>
<html>
<head>
    <link href="~/favicon.ico" rel="icon">
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
    <link href="~/scripts/Login_temp/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Scripts/Admin_temp/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link href="~/scripts/Login_temp/css/style.min.css" rel="stylesheet" />
    @Styles.Render("~/Content/login")
    @Scripts.Render("~/bundles/js")
</head>
<!--Coded with love by Mutiullah Samim-->
<body>
    <div id="particles-js"></div>
        <div class="container h-100">
            <div class="d-flex justify-content-center h-100">
                <div class="user_card content">
                    <div class="d-flex justify-content-center">
                        <div class="brand_logo_container">
                            <img src="~/Scripts/Admin_temp/dist/img/logo.jpg" class="brand_logo" alt="Logo">
                            <p></p>
                            <h6>DataBridge Asia Inc</h6>
                        </div>
                    </div>
                    <div class="form_container">
                        @using (Html.BeginForm("Login", "User", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
                        {
                            <div class="container mx-auto">
                                @*<form>*@
                                <div class="form-group col-12">
                                    <div class="input-group">
                                        <div class="input-group-append">
                                            <span class="input-group-text"><i class="fas fa-user"></i></span>
                                        </div>
                                        @Html.TextBoxFor(m => m.EmailAddress, new { placeholder = " ", @class = "form-control", type = "email", @required = "required" })
                                        @Html.LabelFor(m => m.EmailAddress, new { @class = "form-control-placeholder" })
                                    </div>
                                    @Html.ValidationMessageFor(m => m.EmailAddress, "", new { @class = "" })
                                </div>
                                <div class="form-group col-12">
                                    <div class="input-group">
                                        <div class="input-group-append">
                                            <span class="input-group-text"><i class="fas fa-key"></i></span>
                                        </div>
                                        @Html.TextBoxFor(m => m.Password, new { placeholder = " ", @class = "form-control", type = "password", @required = "required" })
                                        @Html.LabelFor(m => m.Password, new { @class = "form-control-placeholder" })
                                    </div>
                                    @Html.ValidationMessageFor(m => m.Password, "", new { @class = "" })
                                </div>
                                <div class="form-group">
                                    <div class="d-flex justify-content-center mt-3 login_container">
                                        <button type="submit" name="button" class="btn login_btn">Login</button>
                                    </div>
                                </div>
                                @*</form>*@

                            </div>
                        }
                    </div>
                    <div>
                        @*<div class="d-flex justify-content-center links">
                                Don't have an account? <a href="#" class="ml-2">Sign Up</a>
                            </div>*@
                        <div class="d-flex justify-content-center links">
                            <a href="" data-toggle="modal" data-target="#modal-forgotpassword">Forgot your password?</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    <script src="~/scripts/Login_temp/particles.min.js"></script>
    <script src="~/scripts/Login_temp/js/app.min.js"></script>
</body>




@{ Html.RenderPartial("_ForgotPassword");  }

</html>

<style>
    /*Validation CSS*/
    .valid {
        display: block;
        border: solid 1px #28a745;
        padding-right: 2.25rem;
        background-image: url('data:image/jpeg;base64,');
        background-repeat: no-repeat;
        background-position: center right calc(0.375em + 0.1875rem);
        background-size: calc(0.75em + 0.375rem) calc(0.75em + 0.375rem);
    }

    .input-validation-error {
        display: block;
        border: solid 1px #dc3545;
        padding-right: 2.25rem;
        background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' fill='%23dc3545' viewBox='-2 -2 7 7'%3e%3cpath stroke='%23dc3545' d='M0 0l3 3m0-3L0 3'/%3e%3ccircle r='.5'/%3e%3ccircle cx='3' r='.5'/%3e%3ccircle cy='3' r='.5'/%3e%3ccircle cx='3' cy='3' r='.5'/%3e%3c/svg%3E");
        background-repeat: no-repeat;
        background-position: center right calc(0.375em + 0.1875rem);
        background-size: calc(0.75em + 0.375rem) calc(0.75em + 0.375rem);
    }

        .input-validation-error ~ .select2 .select2-selection {
            display: block;
            border: solid 1px #dc3545;
            padding-right: 2.25rem;
            background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' fill='%23dc3545' viewBox='-2 -2 7 7'%3e%3cpath stroke='%23dc3545' d='M0 0l3 3m0-3L0 3'/%3e%3ccircle r='.5'/%3e%3ccircle cx='3' r='.5'/%3e%3ccircle cy='3' r='.5'/%3e%3ccircle cx='3' cy='3' r='.5'/%3e%3c/svg%3E");
            background-repeat: no-repeat;
            background-position: center right calc(0.375em + .6875rem);
            background-size: calc(0.75em + 0.375rem) calc(0.75em + 0.375rem);
        }

    .field-validation-error {
        display: inline-block;
        color: #dc3545;
        font-size: 12px;
    }

        .field-validation-error span::before {
            content: "*";
        }

    .form-control:focus {
        -webkit-box-shadow: none;
        box-shadow: none;
    }


    .form-group {
        position: relative;
        margin-bottom: 1.5rem;
    }

    .form-control-placeholder {
        position: absolute;
        margin: 0px 0 0 50px;
        padding-top: 8.5px;
        transition: all 300ms;
        opacity: 0.6;
        font-size: 15px;
        font-weight: normal;
        font-family: sans-serif;
        cursor: text;
        color: black;
    }

    .form-control:focus ~ .form-control-placeholder,
    .form-control:valid ~ .form-control-placeholder,
    .form-control:invalid:not(:placeholder-shown) ~ .form-control-placeholder,
    .form-control:invalid:not(:empty) ~ .form-control-placeholder,
    .form-control:valid:not([value='']) ~ .form-control-placeholder {
        font-size: 75%;
        transform: translate3d(0, -80%, 0);
        opacity: 1;
        font-weight: normal;
        font-family: sans-serif;
        content: " ";
        display: block;
        position: absolute;
        background: white;
        line-height: 1px;
        z-index: 10;
    }
</style>
