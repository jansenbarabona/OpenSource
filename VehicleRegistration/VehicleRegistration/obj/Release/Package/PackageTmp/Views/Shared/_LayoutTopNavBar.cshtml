    }

    .tl {
        top: 0;
        left: 0;
        border-radius: 28px 0 0 0;
    }

    .tr {
        top: 0;
        right: 0;
        border-radius: 0 28px 0 0;
    }

    .bl {
        bottom: 0;
        left: 0;
        border-radius: 0 0 0 28px;
    }

    .br {
        bottom: 0;
        right: 0;
        border-radius: 0 0 28px 0;
    }


    .tl:before {
        content: "+";
